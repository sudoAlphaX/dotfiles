{
  "Inline Math": {
    "prefix": [
      "$",
      "\\inline",
      "\\mathinline"
    ],
    "body": "$$1$$0",
    "description": "Insert inline math"
  },
  "Math": {
    "prefix": [
      "$$",
      "\\math",
      "\\mathcentered"
    ],
    "body": "$$$1$$$0",
    "description": "Insert centered math environment"
  },
  "Multiplication Symbol": {
    "prefix": [
      "\\times",
      "\\multiply",
      "\\into"
    ],
    "body": "\\times ",
    "description": "Insert Multiplicaton symbol"
  },
  "Division Symbol": {
    "prefix": [
      "\\div",
      "\\division",
      "\\by"
    ],
    "body": "\\div ",
    "description": "Insert Division symbol"
  },
  "Power": {
    "prefix": [
      "\\power",
      "\\raise",
      "\\raised",
      "\\superscript"
    ],
    "body": "^{$1}$0 ",
    "description": "Insert Superscript"
  },
  "Base": {
    "prefix": [
      "\\base",
      "\\below",
      "\\subscript"
