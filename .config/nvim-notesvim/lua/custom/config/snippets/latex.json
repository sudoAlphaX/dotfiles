{
  "Boilerplate": {
    "prefix": [
      "\\boilerplate"
    ],
    "body": [
      "\\documentclass{article}",
      "",
      "% Initialize packages",
      "\\usepackage{geometry}",
      "\\usepackage{tikz}",
      "",
      "% Page Setting",
      "\\geometry{a4paper}",
      "",
      "% Title and Author",
      "\\title{$1}",
      "\\author{$2}",
      "",
      "",
      "\\begin{document}",
      "\\maketitle",
      "",
      "$0",
      "",
      "\\end{document}"
    ],
    "description": "Initialize latex document"
  },
  "Section": {
    "prefix": [
      "#",
      "\\section"
    ],
    "body": [
      "\\section{$1}",
      "",
      "$0"
    ],
    "description": "Insert a section"
  },
  "Subsection": {
    "prefix": [
      "##",
      "\\subsection"
    ],
    "body": [
      "\\subsection{$1}",
      "",
      "$0"
    ],
    "description": "Insert a subsection"
  },
  "Subsubsection": {
    "prefix": [
      "###",
      "\\subsubsection"
    ],
    "body": [
      "\\subsubsection{$1}",
      "",
      "$0"
    ],
    "description": "Insert a subsubsection"
  },
  "1x1 Matrix": {
    "prefix": "\\1x1matrix",
    "body": [
      "\\begin{${1:p/b/v}matrix}",
      "  $2 \\\\\\",
      "\\end{${1}matrix}"
    ]
  },
  "1x2 Matrix": {
    "prefix": "\\1x2matrix",
